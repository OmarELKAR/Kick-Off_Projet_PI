{% extends 'backComp.html.twig' %}
{% block main %}
    <div class="w-4/5 m-auto text-center container">
        <div class="py-12 border-b border-gray-200">
            <h1 class="text-6xl pt-12 pb-8">
                Register
            </h1>
            {{ form_start(TeamForm) }}
            {{ form_row(TeamForm.name, {'label': 'Team name : ','class': 'form-control'}) }}
                    <div class="w-40 mt-2">
            {{ form_row(TeamForm.players, {'label': 'Choose Players : '}) }}
</div>

            <button type="submit"
                    class="uppercase btn btn-primary mt-3">
                Register
            </button>
            {{ form_end(TeamForm) }}

        </div>
    </div>
{% endblock %}